(require Class)

(define Point (Class [$x $y $z]
	(define self.x $x)
	(define self.y $y)
	(define self.z $z)
	(define self.str (lambda []
		(string.concat "( " self.x ", " self.y ", " self.z " )")
	))
))

(print "Class:" Class)
(print "Point:" Point)

(define p1 (Point 0 0 0))
(define p2 (Point 10 15 25))
(define p3 (Point -53 11 27))

(print "Points:")
